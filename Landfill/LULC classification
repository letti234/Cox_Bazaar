/***** End of imports. If edited, may not auto-convert in the playground. *****/
// Supervised classification of LULC in BGD 

//BGD shapefile - entire ADM
//var countries = ee.FeatureCollection("USDOS/LSIB_SIMPLE/2017");
//var BGD = countries.filter(ee.Filter.eq('country_na', 'Bangladesh'));

//BGD shapefile - CB, Chittagong, Naikhongchari
var BGD = ee.FeatureCollection("users/salem043/Coastal_BGD")

Map.addLayer(BGD, {}, "Bangladesh", false);
Map.centerObject(BGD, 8);

///Landsat 8 - cloudfree composite of 2020

//Existing algorithm that removes cloud contamination from image
var image = ee.Algorithms.Landsat.simpleComposite({
  collection: l8raw.filterDate('2020-01-01', '2020-12-31'),
  asFloat: true
});

var trueColorVis = {min: 0, max: 0.3, bands: ['B4', "B3", "B2"]};
Map.addLayer(image.clip(BGD), trueColorVis, 'Composite');


//Generate training data
var label = "Class";
var bands = ['B1', 'B2', 'B3', 'B4', 'B5', 'B7']; ///Can use different bands
var input = image.select(bands);

// 0 = Water; 1 = Settlement, 2 = Forest, 3 = Crops, 4 = Bare, 5 = Aquaculture

//Function merges training variables
var training = Settlement.merge(Crop).merge(Water).merge(Forest).merge(Bare).merge(Aqua);

//Overlay the points on the Landsat image
var trainImage = input.sampleRegions({
  collection: training,
  properties: [label],
  scale: 30
});

//print(trainImage); //Landsat values for each training point, and training class

//Accuracy assessment: use 80% for training and 20% for validation
var trainingData = trainImage.randomColumn();
var trainSet = trainingData.filter(ee.Filter.lessThan('random', 0.8));
var testSet = trainingData.filter(ee.Filter.greaterThanOrEquals('random', 0.8));


//CART - classification and regression tree model
var classifier = ee.Classifier.smileCart().train(trainSet, label, bands);

//Classify the image - apply model to Landsat image
var classified = input.classify(classifier);


//define visualization
var landCoverPalette = [
    'blue',
    'red',
    'green',
    'yellow',
    'grey',
    'orange'
];

Map.addLayer(classified.clip(BGD), {palette:landCoverPalette, min:0, max:5}, "LULC")